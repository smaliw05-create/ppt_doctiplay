    href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;600;800;900&family=IBM+Plex+Mono:wght@400;600&display=swap"
    rel="stylesheet">
  <link rel="stylesheet" href="src/style.css">
  <link rel="stylesheet" href="src/monitor.css">
  <link rel="stylesheet" href="src/cockpit.css">
  <link rel="stylesheet" href="src/dashboard.css">
</head>

<body>
  <div class="topbar">
    <a href="https://doctiplay.com" target="_blank" class="brand" title="Doctiplay"
      style="text-decoration: none; cursor: pointer;">
      <div class="brand-logo">
        <svg viewBox="0 0 120 40" fill="none">
          <path d="M5 20h15l3-10l5 20l4-15l3 5h10" stroke="#00f2ff" stroke-width="2.5" stroke-linecap="round" />
          <path d="M45 8h10c8 0 12 5 12 12s-4 12-12 12H45V8z" stroke="#00f2ff" stroke-width="2.5"
